{
    "manifest_version": 3,
    "name": "QuickTabs",
    "version": "1.0",
    "permissions": ["tabs", "storage", "bookmarks", "activeTab",  "scripting",
    "windows"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "quicktabName.png",
        "48": "quicktabName.png",
        "128": "quicktabName.png"
      },
      "default_popup": "popup.html"
    },
    "popup_action": {
      "default_title": "Open QuickTabs",
      "default_icon": "quicktabName.png",
      "default_popup": "popup.html"
    }
  }
  